---
import BaseHead from "../components/BaseHead.astro";

const metadata = {
  title: "About OAPF - Empowering Professionals, Building a Better Future",
  description:
    "Learn about the Old Anandians&#x27; Professional Forum (OAPF), a dynamic network of professionals dedicated to innovation, education, and community development. Discover our mission, vision, and impactful initiatives.",
};
---

<!doctype html>
<html
  data-wf-page="66425fce7e4b9e25310dc3b5"
  data-wf-site="65afaef75b2da61f9662587d"
  lang="en"
>
  <head>
    <BaseHead title={metadata.title} description={metadata.description} />
  </head>
  <body>
    <div class="utility-page-wrap progress">
      <div class="utility-page-content password w-password-page w-form">
        <form
          action="/.wf_auth"
          method="post"
          id="email-form"
          name="email-form"
          data-name="Email Form"
          class="utility-page-form w-password-page"
          data-wf-page-id="66425fce7e4b9e25310dc3b5"
          data-wf-element-id="66425fce7e4b9e25310dc3bf00000000000c"
        >
          <div class="pass-hero-wrapper">
            <img
              width="192"
              loading="lazy"
              alt=""
              src="images/hand-small.webp"
            />
          </div><img
            src="images/logo.svg"
            loading="lazy"
            alt="Revox Logo"
            height="28"
            class="m-t-30"
          />
          <h2 class="m-b-20">Hi there!<br />Your data is important to us.</h2>
          <p class="p p-b-25">
            Welcome to our project progress and bug reporting portal! This will
            give you access to track the evolution of the project and report any
            pesky bugs you encounter along the way.
          </p><label for="pass" class="form-label text-left w-password-page"
            >Password</label
          ><input
            class="input w-password-page w-input"
            autofocus="true"
            maxlength="256"
            name="pass"
            data-name="field"
            placeholder="Enter your password"
            type="password"
            id="pass"
          /><input
            type="submit"
            data-wait="Please wait..."
            class="btn-primary w-password-page w-button"
            value="Login"
          />
          <div class="p small m-t-25 hint-text">
            This portal is exclusively for authorized personnel. Access is
            restricted to individuals who have obtained explicit permission.
            Unauthorized entry is strictly prohibited. If you do not have the
            requisite authorization, please exit this portal immediately.<br
            /><br />Copyright Â© 2022, Revox Private Limited and/or its
            subsidiaries or affiliates. All Rights Reserved.
          </div>
          <div class="wrong-pass w-password-page w-form-fail">
            <div class="text-block-4">
              Incorrect password. Please try again.
            </div>
          </div>
          <div style="display:none" class="w-password-page w-embed w-script">
            <input
              type="hidden"
              name="path"
              value="<%WF_FORM_VALUE_PATH%>"
            /><input type="hidden" name="page" value="<%WF_FORM_VALUE_PAGE%>" />
          </div>
          <div style="display:none" class="w-password-page w-embed w-script">
            <script type="application/javascript">
              (function _handlePasswordPageOnload() {
                if (/[?&]e=1(&|$)/.test(document.location.search)) {
                  document.querySelector(
                    ".w-password-page.w-form-fail"
                  ).style.display = "block";
                }
              })();
            </script>
          </div>
        </form>
      </div>
    </div>
    <script
      src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=65afaef75b2da61f9662587d"
      type="text/javascript"
      integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0="
      crossorigin="anonymous"></script>
    <script src="js/webflow.js" type="text/javascript"></script>
  </body>
</html>
